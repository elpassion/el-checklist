{
  "name": "self-organazier",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.1",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --collectCoverageFrom=src/**/*.{js,ts,tsx} --collectCoverageFrom=!src/**/*.d.ts  --collectCoverageFrom=!src/index.tsx",
    "test:watch": "npm run test -- --watchAll --watch",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:js:fix": "eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "ts": "tsc -p ./tsconfig.json",
    "ts:watch": "npm run ts --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "4.0.4",
    "@testing-library/react": "8.0.4",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "cypress": "3.4.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jest": "22.7.2",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1"
  }
}
